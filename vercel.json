{
    "version": 2,
    "builds": [
        {
            "src": "src/bot.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/webhook/.*",
            "dest": "src/bot.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/bot.js"
        }
    ]
}